{
	"name": "discosino",
	"version": "1.0.0",
	"description": "Casino bot for Discord",
	"author": "Henning Blomqvist (https://github.com/hblomqvist)",
	"license": "Apache-2.0",
	"repository": "github:hblomqvist/discosino",
	"private": true,
	"main": "./src/bot.ts",
	"imports": {
		"#lib/*": "./src/lib/*.ts",
		"#lib/env": "./src/lib/env/index.ts",
		"#lib/eval": "./src/lib/eval/index.ts",
		"#lib/extensions": "./src/lib/extensions/index.ts",
		"#util/*": "./src/lib/util/*.ts",
		"#util/sanitizer": "./src/lib/util/sanitizer/index.ts",
		"#util/structures": "./src/lib/util/structures/index.ts",
		"#root/*": "./src/*.ts"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=production ts-node --project ./src/tsconfig.json --transpile-only --files ./src/bot.ts",
		"dev": "ts-node-dev --respawn --project ./src/tsconfig.json --files --clear --exit-child --require dotenv/config ./src/bot.ts",
		"lint": "eslint --fix --ext .ts src/",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.3.3",
		"@sapphire/fetch": "^2.3.0",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.0",
		"@sapphire/stopwatch": "^1.4.0",
		"@sapphire/ts-config": "^3.3.3",
		"@sapphire/type": "^2.2.0",
		"@sapphire/utilities": "^3.6.1",
		"discord.js": "^13.6.0",
		"prettier": "^2.6.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@sapphire/eslint-config": "^4.3.2",
		"@types/node": "^17.0.22",
		"@types/prettier": "^2.4.4",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier-plugin-organize-imports": "^2.3.4",
		"prettier-plugin-prisma": "^3.11.0",
		"prettier-plugin-sh": "^0.8.1",
		"pretty-quick": "^3.1.3",
		"ts-node-dev": "^1.1.8"
	},
	"overrides": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.6"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"lint-staged": {
		"*.ts": "eslint --fix --ext .ts"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
