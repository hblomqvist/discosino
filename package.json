{
	"name": "discosino",
	"version": "0.1.0",
	"description": "Casino bot for Discord",
	"author": "Henning Blomqvist",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/hblomqvist/discosino.git"
	},
	"private": true,
	"type": "module",
	"main": "./dist/discosino.js",
	"imports": {
		"#core/*": "./dist/lib/core/*.js",
		"#core/framework": "./dist/lib/core/framework/index.js",
		"#feat/*": "./dist/lib/feat/*.js",
		"#feat/eval": "./dist/lib/feat/eval/index.js",
		"#util/*": "./dist/lib/util/*.js",
		"#util/structures": "./dist/lib/util/structures/index.js",
		"#config": "./dist/config.js"
	},
	"scripts": {
		"start": "node --enable-source-maps --experimental-specifier-resolution=node . --color=16m",
		"start:migrate": "prisma migrate deploy && yarn start",
		"dev": "tsup --onSuccess \"yarn start\"",
		"watch": "tsup --watch --onSuccess \"yarn start\"",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "eslint --fix .",
		"format": "prettier --write .",
		"clean": "rimraf dist",
		"reinstall": "rimraf node_modules yarn.lock && yarn install",
		"postinstall": "node scripts/postinstall.js"
	},
	"dependencies": {
		"@prisma/client": "^4.5.0",
		"@sapphire/decorators": "^5.0.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^3.1.3",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/stopwatch": "^1.5.0",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.11.0",
		"boxen": "^7.0.0",
		"colorette": "^2.0.19",
		"discord-api-types": "^0.33.5",
		"discord.js": "^13.12.0",
		"dotenv": "^16.0.3",
		"envalid": "^7.3.1",
		"figlet": "^1.5.2",
		"fuse.js": "^6.6.2",
		"gradient-string": "^2.0.2",
		"prettier": "^2.7.1",
		"prisma": "^4.5.0",
		"treeify": "^1.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/ts-config": "^3.3.4",
		"@types/figlet": "^1.5.5",
		"@types/gradient-string": "^1.1.2",
		"@types/node": "^18.11.9",
		"@types/prettier": "^2.7.1",
		"@types/treeify": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier-plugin-organize-imports": "^3.1.1",
		"prettier-plugin-prisma": "^4.4.0",
		"prettier-plugin-sh": "^0.12.8",
		"pretty-quick": "^3.1.3",
		"rimraf": "^3.0.2",
		"tsup": "^6.4.0",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": ">=18"
	},
	"volta": {
		"node": "18.12.0",
		"yarn": "1.22.19"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@3.2.4"
}
